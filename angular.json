{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "track-cargo": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/track-cargo",
      "sourceRoot": "apps/track-cargo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": ["chart.js"],
            "outputPath": "dist/apps/track-cargo",
            "index": "apps/track-cargo/src/index.html",
            "main": "apps/track-cargo/src/main.ts",
            "polyfills": "apps/track-cargo/src/polyfills.ts",
            "tsConfig": "apps/track-cargo/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/track-cargo/src/favicon.ico",
              "apps/track-cargo/src/assets"
            ],
            "styles": [
              "apps/track-cargo/src/styles.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "apps/track-cargo/src/assets/styles/tailwind.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/track-cargo/src/environments/environment.ts",
                  "with": "apps/track-cargo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "track-cargo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "track-cargo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "track-cargo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/track-cargo/src/test.ts",
            "polyfills": "apps/track-cargo/src/polyfills.ts",
            "tsConfig": "apps/track-cargo/tsconfig.spec.json",
            "karmaConfig": "apps/track-cargo/karma.conf.js",
            "assets": [
              "apps/track-cargo/src/favicon.ico",
              "apps/track-cargo/src/assets"
            ],
            "styles": ["apps/track-cargo/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/track-cargo/tsconfig.app.json",
              "apps/track-cargo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "track-cargo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "track-cargo:serve:production"
            }
          }
        }
      }
    },
    "track-cargo-e2e": {
      "root": "apps/track-cargo-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/track-cargo-e2e/protractor.conf.js",
            "devServerTarget": "track-cargo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "track-cargo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/track-cargo-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "track-cargo",
  "cli": {
    "defaultCollection": "@nrwl/angular"
  }
}
